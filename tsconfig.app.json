{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["ES2022","DOM"],
    "moduleResolution": "Node",
    "importHelpers": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "strict": false,
    "useDefineForClassFields": false,
    "baseUrl": "./",
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": ["src/main.ts","src/polyfills.ts"],
  "include": ["src/**/*.d.ts"]
}
